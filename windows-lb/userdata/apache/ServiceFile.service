[Unit]  
Description=ASP

[Service]  
WorkingDirectory=/var/www/testgit  
ExecStart=/usr/bin/dotnet /var/www/testgit/testgit.dll  
Restart=always  
RestartSec=10
KillSignal=SIGINT  
SyslogIdentifier=netcore-demo 
User=www-data  
Environment=ASPNETCORE_ENVIRONMENT=Production  
[Install]  
WantedBy=multi-user.target