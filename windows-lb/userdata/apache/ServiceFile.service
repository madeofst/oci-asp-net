[Unit]
Description=ASP

[Service]
WorkingDirectory=/var/www/webapp
ExecStart=/home/ubuntu/dotnet-64/dotnet /var/www/webapp/webapp.dll
Restart=always
RestartSec=10
KillSignal=SIGINT
SyslogIdentifier=netcore-demo
User=ubuntu
Environment=ASPNETCORE_ENVIRONMENT=Production

[Install]
WantedBy=multi-user.target
